{namespace hs.tmpl}

/**
* Renders the Action Bar
*/
{template .ActionBar}
    <div id="actionbar">
        <a href="#/">
            <img src="img/ab_logo.png" alt="connectsy" id="ab_logo" />
        </a>
    </div>
{/template}

/**
* Renders the main HTML body
*/
{template .Main}
    {call .ActionBar data="all"/}
    <div id="main"></div>
{/template}

