<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap="http://phonegap.com/ns/1.0"
        id="com.hipsell.Hipsell"
        version="0.1.0">

    <name>Hipsell</name>
    <description>Sell shit quick.</description>

    <author href="http://hipsell.com" email="info@hipsell.com">
        Hipsell
    </author>

    <gap:platforms>
        <gap:platform name="android" minVersion="2.1" />
    </gap:platforms>

    <icon src="img/icon.png" gap:role="default" />
    <icon src="img/icon.android.ldpi.png" width="36" height="36" />
    <icon src="img/icon.android.mdpi.png" width="48" height="48" />
    <icon src="img/icon.android.hdpi.png" width="72" height="72" />
    <icon src="img/icon.ios.retna.png" width="114" height="114" />
    <icon src="img/icon.ios.low.png" width="57" height="57" />

    <feature name="http://api.phonegap.com/1.0/geolocation"/>
    <feature name="http://api.phonegap.com/1.0/network"/>
    <feature name="http://api.phonegap.com/1.0/camera"/>
</widget>